@model JwtAuthorization.Models.ForgotPasswordViewModel

<h2>Forgot Password</h2>

<form method="post">
    <input asp-for="Email" class="form-control" placeholder="Enter Email" />
    <button class="btn btn-warning mt-2">Send Reset Link</button>
</form>

<p class="text-success">@ViewBag.Message</p>
